{
  "name": "ai-powered-portfolio",
  "version": "1.0.0",
  "description": "AI-powered professional portfolio with FastAPI backend and React frontend",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:json": "prettier --write '**/*.json'",
    "format:json:check": "prettier --check '**/*.json'",
    "lint:python": "cd backend && flake8 .",
    "format:python": "cd backend && black . && isort .",
    "format:python:check": "cd backend && black --check . && isort --check-only .",
    "lint:all": "npm run lint && npm run lint:python",
    "format:all": "npm run format && npm run format:python",
    "check:all": "npm run format:check && npm run format:python:check && npm run lint",
    "test:backend": "cd backend && python -m pytest tests/ -v",
    "dev:backend": "cd backend && python run.py"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "portfolio",
    "ai",
    "fastapi",
    "react",
    "typescript"
  ],
  "author": "Taylor Kuno",
  "license": "MIT"
}
